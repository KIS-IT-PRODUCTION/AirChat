{
  "expo": {
    "name": "AirChat",
    "slug": "AirChat",
    "scheme": "airchat",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.airchat.mobile",
      "config": {
        "googleMapsApiKey": "AIzaSyAKwWqSjapoyrIBnAxnbByX6PMJZWGgzlo"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "tel",
          "sms"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/apdative-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.airchat.mobile",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAKwWqSjapoyrIBnAxnbByX6PMJZWGgzlo"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "Додатку потрібен доступ до вашої галереї, щоб ви могли надсилати фото.",
          "cameraPermission": "Додатку потрібен доступ до вашої камери, щоб ви могли робити та надсилати фото."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Цей застосунок потребує доступу до вашої геолокації, щоб ділитися нею з друзями."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon2.png",
          "color": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "e5ae05a0-322d-4a51-84d9-84738230258b"
      }
    }
  }
}